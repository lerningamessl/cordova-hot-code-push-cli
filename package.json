    "hidefile": "~3.0.0",
    "lodash": "~4.17.15",
    "minimatch": "^3.0.0",
    "ngrok": "~3.2.5",
    "prompt": "~1.0.0",
    "q": "^1.4.1",
    "readdirp": "~3.2.0",
    "recursive-readdir": "^2.0.0",
    "s3-sync-aws": "git+https://github.com/lerningamessl/s3-sync-aws.git#",
