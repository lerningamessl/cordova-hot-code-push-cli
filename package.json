{
  "name": "cordova-hot-code-push-cli",
  "version": "1.1.1",
  "description": "Cordova Hot Code Push CLI",
  "preferGlobal": "true",
  "bin": {
    "cordova-hcp": "bin/cordova-hcp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nordnet/cordova-hot-code-push-cli.git"
  },
  "keywords": [
    "cordova",
    "nordnet",
    "mobile",
    "cordova-hot-code-push-plugin",
    "ecosystem:cordova",
